pipeline
{

agent {
  label 'server1'
}

tools {
  maven 'maven3'
}

stages{

    stage('build')
    {
        steps {
            sh 'cd simple-java-maven-app && mvn clean package'
        }

        post {
        success {
            archiveArtifacts artifacts: '**/target/*.war'
                 }
            }

    }

   

    
}

}